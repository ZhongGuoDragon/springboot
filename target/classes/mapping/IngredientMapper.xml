<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tom.springboot.mapper.IngredientMapper">
    <resultMap id="ingredientList" type="com.tom.springboot.pojo.Ingredient">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="type" property="type"/>
    </resultMap>
    <select id="getIngredientList" resultMap="ingredientList" >
        SELECT * FROM springboot.ingredient;
    </select>
</mapper>
